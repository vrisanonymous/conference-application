pipeline {
    agent any
    stages {
        stage('Build') {
           steps{
               echo "build"
               sh "mvn clean package checkstyle:checkstyle"
           }
        }

        stage('CheckStyle') {
            steps{
                echo "checkstyle"
                sh "mvn clean package checkstyle:checkstyle"
                }
        }

        stage('Test') {
           steps{
               echo "test"
           }
        }
        stage('Deploy') {
           steps{
               echo "deploy"
           }
        }
    }
}